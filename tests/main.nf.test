nextflow_pipeline {

    name "Test Workflow main.nf"
    script "main.nf"
    tag "PIPELINE"

    test("Default test for reproducibility") {

        when {
            params {
                outdir = "$outputDir"
                // skip the check module step for now maybe change it in the future
                check_model = false
            }
        }

        then {
            assert workflow.success
        }
    }
}
